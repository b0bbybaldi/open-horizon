{
  "org": "dcmartin@us.ibm.com",
  "label": "hal",
  "description": "Provides hardware abstraction layer as service",
  "documentation": "https://github.com/dcmartin/open-horizon/hal/README.md",
  "url": "https://github.com/dcmartin/open-horizon/hal",
  "version": "0.0.1",
  "arch": "amd64",
  "public": true,
  "sharable": "multiple",
  "requiredServices": [],
  "userInput": [],
  "deployment": {
    "services": {
      "hal": {
        "image": "dcmartin/amd64_hal:0.0.1",
        "privileged": true
      }
    }
  },
  "ports": {
    "8585/tcp": 8585 
  },
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)"
  }
}
