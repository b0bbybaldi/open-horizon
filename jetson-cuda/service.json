{
  "org": "dcmartin@us.ibm.com",
  "label": "jetson-cuda",
  "description": "Provide CUDA on Jetson TX2 with JetPack v 2.3.3",
  "documentation": "https://github.com/dcmartin/open-horizon/jetson-cuda/README.md",
  "url": "com.github.dcmartin.open-horizon.jetson-cuda",
  "version": "0.0.1",
  "arch": null,
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [
    { "name": "DEBUG", "label": "debug on/off", "type": "boolean", "defaultValue": "false" }
  ],
  "deployment": {
    "services": {
      "jetson-cuda": {
        "environment": [
          "SERVICE_LABEL=jetson-cuda","SERVICE_VERSION=0.0.1"
        ],
        "specific_ports": [],
        "image": null,
        "privileged": true
      }
    }
  },
  "tmpfs": {
    "size": 2048000
  },
  "ports": {
    "80/tcp": 8681
  }
}
