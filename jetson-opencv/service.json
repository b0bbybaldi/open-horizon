{
  "org": "${HZN_ORG_ID}",
  "label": "jetson-opencv",
  "description": "Provide OpenCV on Jetson TX2 with JetPack and CUDA",
  "documentation": "https://github.com/dcmartin/open-horizon/jetson-opencv/README.md",
  "url": "com.github.dcmartin.open-horizon.jetson-opencv",
  "version": "0.0.1",
  "arch": null,
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [
    {
      "name": "DEBUG",
      "label": "debug on/off",
      "type": "boolean",
      "defaultValue": "false"
    }
  ],
  "deployment": {
    "services": {
      "jetson-opencv": {
        "environment": [
          "SERVICE_LABEL=jetson-opencv",
          "SERVICE_VERSION=0.0.1"
        ],
        "specific_ports": [],
        "image": null,
        "privileged": true
      }
    }
  },
  "tmpfs": {
    "size": 2048000
  },
  "ports": {
    "80/tcp": 8682
  }
}
